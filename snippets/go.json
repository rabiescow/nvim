{
  "pac + pack + packa + packag + package + package \"\"": {
    "body": "package $1",
    "prefix": [
      "pac",
      "pack",
      "packa",
      "packag",
      "package",
      "package "
    ]
  },
  "pkg": {
    "body": [
      "package main",
      "",
      "func main() {",
      "    $1",
      "}"
    ],
    "prefix": "main"
  }
}
